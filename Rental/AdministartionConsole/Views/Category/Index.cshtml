@model IEnumerable<AdministartionConsole.Models.Dto.CategoryDto>
@{
    ViewData["Title"] = "Category lists";
}

<h1>Category List:</h1>

@Html.ActionLink("Add", "Create", "Category")

<div>
    <hr />
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
                </tr>
            }
        </table>
</div>